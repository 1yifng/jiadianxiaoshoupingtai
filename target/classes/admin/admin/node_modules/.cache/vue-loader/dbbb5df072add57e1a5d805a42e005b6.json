{"remainingRequest":"C:\\jiadianxiaoshoupingtai\\src\\main\\resources\\admin\\admin\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\jiadianxiaoshoupingtai\\src\\main\\resources\\admin\\admin\\src\\views\\modules\\jiadianOrder\\list.vue?vue&type=style&index=0&id=ff745cde&prod&scoped=true&lang=css","dependencies":[{"path":"C:\\jiadianxiaoshoupingtai\\src\\main\\resources\\admin\\admin\\src\\views\\modules\\jiadianOrder\\list.vue","mtime":1746128613268},{"path":"C:\\jiadianxiaoshoupingtai\\src\\main\\resources\\admin\\admin\\node_modules\\css-loader\\dist\\cjs.js","mtime":1743781065361},{"path":"C:\\jiadianxiaoshoupingtai\\src\\main\\resources\\admin\\admin\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":1743781065757},{"path":"C:\\jiadianxiaoshoupingtai\\src\\main\\resources\\admin\\admin\\node_modules\\postcss-loader\\src\\index.js","mtime":1743781065333},{"path":"C:\\jiadianxiaoshoupingtai\\src\\main\\resources\\admin\\admin\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1743781066581},{"path":"C:\\jiadianxiaoshoupingtai\\src\\main\\resources\\admin\\admin\\node_modules\\vue-loader\\lib\\index.js","mtime":1743781065439}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["list.vue"],"names":[],"mappings":";AAqmBA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA","file":"list.vue","sourceRoot":"src/views/modules/jiadianOrder","sourcesContent":["<template>\r\n  <!-- 主内容容器 -->\r\n  <div class=\"main-content\">\r\n    <!-- 条件查询区域 -->\r\n    <div v-if=\"showFlag\">\r\n      <!-- 搜索表单 -->\r\n      <el-form :inline=\"true\" :model=\"searchForm\" class=\"form-content\">\r\n        <!-- 搜索条件行 -->\r\n        <el-row :gutter=\"20\" class=\"slt\">\r\n          <!-- 订单号搜索框 -->\r\n          <el-form-item label=\"订单号\">\r\n            <el-input\r\n                prefix-icon=\"el-icon-search\"\r\n                v-model=\"searchForm.jiadianOrderUuidNumber\"\r\n                placeholder=\"订单号\"\r\n                clearable>\r\n            </el-input>\r\n          </el-form-item>\r\n\r\n          <!-- 订单类型下拉选择 -->\r\n          <el-form-item label=\"订单类型\">\r\n            <el-select v-model=\"searchForm.jiadianOrderTypes\" placeholder=\"请选择订单类型\">\r\n              <el-option label=\"=-请选择-=\" value=\"\"></el-option>\r\n              <el-option\r\n                  v-for=\"(item,index) in jiadianOrderTypesSelectSearch\"\r\n                  :key=\"index\"\r\n                  :label=\"item.indexName\"\r\n                  :value=\"item.codeIndex\">\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n\r\n          <!-- 商品名称搜索框 -->\r\n          <el-form-item label=\"商品名称\">\r\n            <el-input\r\n                prefix-icon=\"el-icon-search\"\r\n                v-model=\"searchForm.jiadianName\"\r\n                placeholder=\"商品名称\"\r\n                clearable>\r\n            </el-input>\r\n          </el-form-item>\r\n\r\n          <!-- 商品类型下拉选择 -->\r\n          <el-form-item label=\"商品类型\">\r\n            <el-select v-model=\"searchForm.jiadianTypes\" placeholder=\"请选择商品类型\">\r\n              <el-option label=\"=-请选择-=\" value=\"\"></el-option>\r\n              <el-option\r\n                  v-for=\"(item,index) in jiadianTypesSelectSearch\"\r\n                  :key=\"index\"\r\n                  :label=\"item.indexName\"\r\n                  :value=\"item.codeIndex\">\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n\r\n          <!-- 用户姓名搜索框 -->\r\n          <el-form-item label=\"用户姓名\">\r\n            <el-input\r\n                prefix-icon=\"el-icon-search\"\r\n                v-model=\"searchForm.yonghuName\"\r\n                placeholder=\"用户姓名\"\r\n                clearable>\r\n            </el-input>\r\n          </el-form-item>\r\n\r\n          <!-- 查询按钮 -->\r\n          <el-form-item>\r\n            <el-button\r\n                type=\"success\"\r\n                @click=\"search()\">\r\n              查询<i class=\"el-icon-search el-icon--right\"/>\r\n            </el-button>\r\n          </el-form-item>\r\n        </el-row>\r\n\r\n        <!-- 操作按钮行 -->\r\n        <el-row class=\"ad\">\r\n          <el-form-item>\r\n            <!-- 删除按钮 -->\r\n            <el-button\r\n                :disabled=\"dataListSelections.length <= 0\"\r\n                type=\"danger\"\r\n                icon=\"el-icon-delete\"\r\n                @click=\"deleteHandler()\">\r\n              删除\r\n            </el-button>\r\n          </el-form-item>\r\n        </el-row>\r\n      </el-form>\r\n\r\n      <!-- 表格内容区域 -->\r\n      <div class=\"table-content\">\r\n        <!-- 商品订单数据表格 -->\r\n        <el-table\r\n            class=\"tables\"\r\n            :data=\"dataList\"\r\n            v-loading=\"dataListLoading\"\r\n            @selection-change=\"selectionChangeHandler\"\r\n            stripe\r\n            border\r\n            fit>\r\n\r\n          <!-- 选择列 -->\r\n          <el-table-column\r\n              type=\"selection\"\r\n              header-align=\"center\"\r\n              align=\"center\"\r\n              width=\"50\">\r\n          </el-table-column>\r\n\r\n          <!-- 收货人列 -->\r\n          <el-table-column\r\n              prop=\"addressName\"\r\n              header-align=\"center\"\r\n              label=\"收货人\">\r\n            <template slot-scope=\"scope\">\r\n              {{scope.row.addressName}}\r\n            </template>\r\n          </el-table-column>\r\n\r\n          <!-- 电话列 -->\r\n          <el-table-column\r\n              prop=\"addressPhone\"\r\n              header-align=\"center\"\r\n              label=\"电话\">\r\n            <template slot-scope=\"scope\">\r\n              {{scope.row.addressPhone}}\r\n            </template>\r\n          </el-table-column>\r\n\r\n          <!-- 地址列 -->\r\n          <el-table-column\r\n              prop=\"addressDizhi\"\r\n              header-align=\"center\"\r\n              label=\"地址\">\r\n            <template slot-scope=\"scope\">\r\n              {{scope.row.addressDizhi}}\r\n            </template>\r\n          </el-table-column>\r\n\r\n          <!-- 商品名称列 -->\r\n          <el-table-column\r\n              prop=\"jiadianName\"\r\n              header-align=\"center\"\r\n              label=\"商品名称\">\r\n            <template slot-scope=\"scope\">\r\n              {{scope.row.jiadianName}}\r\n            </template>\r\n          </el-table-column>\r\n\r\n          <!-- 商品照片列 -->\r\n          <el-table-column\r\n              prop=\"jiadianPhoto\"\r\n              header-align=\"center\"\r\n              width=\"200\"\r\n              label=\"商品照片\">\r\n            <template slot-scope=\"scope\">\r\n              <div v-if=\"scope.row.jiadianPhoto\">\r\n                <img :src=\"$base.url+scope.row.jiadianPhoto\" width=\"100\" height=\"100\">\r\n              </div>\r\n              <div v-else>无图片</div>\r\n            </template>\r\n          </el-table-column>\r\n\r\n          <!-- 商品类型列 -->\r\n          <el-table-column\r\n              prop=\"jiadianTypes\"\r\n              header-align=\"center\"\r\n              label=\"商品类型\">\r\n            <template slot-scope=\"scope\">\r\n              {{scope.row.jiadianValue}}\r\n            </template>\r\n          </el-table-column>\r\n\r\n          <!-- 用户姓名列 -->\r\n          <el-table-column\r\n              prop=\"yonghuName\"\r\n              header-align=\"center\"\r\n              label=\"用户姓名\">\r\n            <template slot-scope=\"scope\">\r\n              {{scope.row.yonghuName}}\r\n            </template>\r\n          </el-table-column>\r\n\r\n          <!-- 订单号列 -->\r\n          <el-table-column\r\n              prop=\"jiadianOrderUuidNumber\"\r\n              header-align=\"center\"\r\n              label=\"订单号\">\r\n            <template slot-scope=\"scope\">\r\n              {{scope.row.jiadianOrderUuidNumber}}\r\n            </template>\r\n          </el-table-column>\r\n\r\n          <!-- 购买数量列 -->\r\n          <el-table-column\r\n              prop=\"buyNumber\"\r\n              header-align=\"center\"\r\n              label=\"购买数量\">\r\n            <template slot-scope=\"scope\">\r\n              {{scope.row.buyNumber}}\r\n            </template>\r\n          </el-table-column>\r\n\r\n          <!-- 实付价格列 -->\r\n          <el-table-column\r\n              prop=\"jiadianOrderTruePrice\"\r\n              header-align=\"center\"\r\n              label=\"实付价格\">\r\n            <template slot-scope=\"scope\">\r\n              {{scope.row.jiadianOrderTruePrice}}\r\n            </template>\r\n          </el-table-column>\r\n\r\n          <!-- 快递公司列 -->\r\n          <el-table-column\r\n              prop=\"jiadianOrderCourierName\"\r\n              header-align=\"center\"\r\n              label=\"快递公司\">\r\n            <template slot-scope=\"scope\">\r\n              {{scope.row.jiadianOrderCourierName}}\r\n            </template>\r\n          </el-table-column>\r\n\r\n          <!-- 快递单号列 -->\r\n          <el-table-column\r\n              prop=\"jiadianOrderCourierNumber\"\r\n              header-align=\"center\"\r\n              label=\"订单快递单号\">\r\n            <template slot-scope=\"scope\">\r\n              {{scope.row.jiadianOrderCourierNumber}}\r\n            </template>\r\n          </el-table-column>\r\n\r\n          <!-- 订单类型列 -->\r\n          <el-table-column\r\n              prop=\"jiadianOrderTypes\"\r\n              header-align=\"center\"\r\n              label=\"订单类型\">\r\n            <template slot-scope=\"scope\">\r\n              {{scope.row.jiadianOrderValue}}\r\n            </template>\r\n          </el-table-column>\r\n\r\n          <!-- 订单创建时间列 -->\r\n          <el-table-column\r\n              prop=\"insertTime\"\r\n              header-align=\"center\"\r\n              label=\"订单创建时间\">\r\n            <template slot-scope=\"scope\">\r\n              {{scope.row.insertTime}}\r\n            </template>\r\n          </el-table-column>\r\n\r\n          <!-- 操作列 -->\r\n          <el-table-column\r\n              width=\"300\"\r\n              align=\"center\"\r\n              header-align=\"center\"\r\n              label=\"操作\">\r\n            <template slot-scope=\"scope\">\r\n              <!-- 详情按钮 -->\r\n              <el-button\r\n                  type=\"success\"\r\n                  icon=\"el-icon-tickets\"\r\n                  size=\"mini\"\r\n                  @click=\"addOrUpdateHandler(scope.row.id,'info')\">\r\n                详情\r\n              </el-button>\r\n\r\n              <!-- 删除按钮 -->\r\n              <el-button\r\n                  type=\"danger\"\r\n                  icon=\"el-icon-delete\"\r\n                  size=\"mini\"\r\n                  @click=\"deleteHandler(scope.row.id)\">\r\n                删除\r\n              </el-button>\r\n\r\n              <!-- 发货按钮（订单状态为待发货） -->\r\n              <el-button\r\n                  v-if=\"scope.row.jiadianOrderTypes == 101 && sessionTable=='users'\"\r\n                  type=\"primary\"\r\n                  icon=\"el-icon-reading\"\r\n                  size=\"mini\"\r\n                  @click=\"openDeliver(scope.row.id)\">\r\n                发货\r\n              </el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n\r\n        <!-- 分页组件 -->\r\n        <el-pagination\r\n            class=\"pagination-content\"\r\n            @size-change=\"sizeChangeHandle\"\r\n            @current-change=\"currentChangeHandle\"\r\n            :current-page=\"pageIndex\"\r\n            :page-sizes=\"[10]\"\r\n            :page-size=\"pageSize\"\r\n            :total=\"totalPage\"\r\n            layout=\"total, sizes, prev, pager, next, jumper\"\r\n            background>\r\n        </el-pagination>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 添加/修改页面组件 -->\r\n    <add-or-update\r\n        v-if=\"addOrUpdateFlag\"\r\n        :parent=\"this\"\r\n        ref=\"addOrUpdate\">\r\n    </add-or-update>\r\n\r\n    <!-- 发货弹窗 -->\r\n    <el-dialog\r\n        title=\"发货\"\r\n        :visible.sync=\"deliverVisible\"\r\n        width=\"30%\">\r\n      <el-form>\r\n        <el-form-item label=\"快递公司\">\r\n          <el-input\r\n              v-model=\"jiadianOrderCourierName\"\r\n              autocomplete=\"off\">\r\n          </el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"订单快递单号\">\r\n          <el-input\r\n              v-model=\"jiadianOrderCourierNumber\"\r\n              autocomplete=\"off\">\r\n          </el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"deliverVisible = false\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"deliver()\">发 货</el-button>\r\n            </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// 导入添加/修改组件\r\nimport AddOrUpdate from \"./add-or-update\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      // 发货相关数据\r\n      deliverId: null,      // 发货商品ID\r\n      jiadianOrderCourierNumber: null,  // 快递单号\r\n      jiadianOrderCourierName: null,    // 快递公司\r\n      deliverVisible: false,            // 发货弹窗显示状态\r\n\r\n      // 搜索表单数据\r\n      searchForm: {\r\n        key: \"\",\r\n        jiadianOrderUuidNumber: \"\",   // 订单号\r\n        jiadianOrderTypes: \"\",        // 订单类型\r\n        jiadianName: \"\",              // 商品名称\r\n        jiadianTypes: \"\",             // 商品类型\r\n        yonghuName: \"\"                // 用户姓名\r\n      },\r\n\r\n      // 用户信息\r\n      sessionTable: \"\",  // 登录账户所在表名\r\n      role: \"\",          // 权限\r\n      userId: \"\",        // 当前登录人的id\r\n\r\n      // 下拉框选项\r\n      jiadianTypesSelectSearch: [],         // 商品类型下拉选项\r\n      jiadianOrderTypesSelectSearch: [],    // 订单类型下拉选项\r\n\r\n      // 表格数据\r\n      dataList: [],           // 订单数据列表\r\n      pageIndex: 1,           // 当前页码\r\n      pageSize: 10,           // 每页条数\r\n      totalPage: 0,           // 总条数\r\n      dataListLoading: false, // 表格加载状态\r\n      dataListSelections: [], // 表格选中项\r\n\r\n      // 页面显示控制\r\n      showFlag: true,         // 是否显示列表页\r\n      addOrUpdateFlag: false  // 是否显示添加/修改页\r\n    };\r\n  },\r\n  components: {\r\n    AddOrUpdate\r\n  },\r\n  created() {\r\n    this.init();            // 初始化\r\n    this.getDataList();     // 获取数据列表\r\n\r\n    // 获取当前登录用户信息\r\n    this.sessionTable = this.$storage.get(\"sessionTable\");\r\n    this.role = this.$storage.get(\"role\");\r\n    this.userId = this.$storage.get(\"userId\");\r\n  },\r\n  methods: {\r\n\r\n    /**\r\n     * 搜索方法\r\n     */\r\n    search() {\r\n      this.pageIndex = 1;\r\n      this.getDataList();\r\n    },\r\n\r\n    /**\r\n     * 获取数据列表\r\n     */\r\n    getDataList() {\r\n      this.dataListLoading = true;\r\n      let params = {\r\n        page: this.pageIndex,\r\n        limit: this.pageSize,\r\n        sort: 'id',\r\n      };\r\n\r\n      // 添加搜索条件\r\n      if (this.searchForm.jiadianName) {\r\n        params['jiadianName'] = '%' + this.searchForm.jiadianName + '%';\r\n      }\r\n      if (this.searchForm.jiadianTypes) {\r\n        params['jiadianTypes'] = this.searchForm.jiadianTypes;\r\n      }\r\n      if (this.searchForm.yonghuName) {\r\n        params['yonghuName'] = '%' + this.searchForm.yonghuName + '%';\r\n      }\r\n      if (this.searchForm.jiadianOrderUuidNumber) {\r\n        params['jiadianOrderUuidNumber'] = '%' + this.searchForm.jiadianOrderUuidNumber + '%';\r\n      }\r\n      if (this.searchForm.jiadianOrderTypes) {\r\n        params['jiadianOrderTypes'] = this.searchForm.jiadianOrderTypes;\r\n      }\r\n\r\n      // 只查询未删除的数据\r\n      params['jiadianOrderDelete'] = 1;\r\n\r\n      // 请求订单数据\r\n      this.$http({\r\n        url: \"jiadianOrder/page\",\r\n        method: \"get\",\r\n        params: params\r\n      }).then(({data}) => {\r\n        if(data && data.code === 0){\r\n          this.dataList = data.data.list;\r\n          this.totalPage = data.data.total;\r\n        }else{\r\n          this.dataList = [];\r\n          this.totalPage = 0;\r\n        }\r\n        this.dataListLoading = false;\r\n      });\r\n\r\n      // 获取商品类型下拉选项\r\n      this.$http({\r\n        url: \"dictionary/page?dicCode=jiadian_types&page=1&limit=100\",\r\n        method: \"get\"\r\n      }).then(({data}) => {\r\n        if(data && data.code === 0){\r\n          this.jiadianTypesSelectSearch = data.data.list;\r\n        }\r\n      });\r\n\r\n      // 获取订单类型下拉选项\r\n      this.$http({\r\n        url: \"dictionary/page?dicCode=jiadian_order_types&page=1&limit=100\",\r\n        method: \"get\"\r\n      }).then(({data}) => {\r\n        if(data && data.code === 0){\r\n          this.jiadianOrderTypesSelectSearch = data.data.list;\r\n        }\r\n      });\r\n    },\r\n\r\n    /**\r\n     * 每页数量变化处理方法\r\n     * @param {number} val - 新的每页条数\r\n     */\r\n    sizeChangeHandle(val) {\r\n      this.pageSize = val;\r\n      this.pageIndex = 1;\r\n      this.getDataList();\r\n    },\r\n\r\n    /**\r\n     * 当前页变化处理方法\r\n     * @param {number} val - 新的页码\r\n     */\r\n    currentChangeHandle(val) {\r\n      this.pageIndex = val;\r\n      this.getDataList();\r\n    },\r\n\r\n    /**\r\n     * 表格选择项变化处理方法\r\n     * @param {array} val - 选中的行数据\r\n     */\r\n    selectionChangeHandler(val) {\r\n      this.dataListSelections = val;\r\n    },\r\n\r\n    /**\r\n     * 添加/修改处理方法\r\n     * @param {number|string} id - 记录ID\r\n     * @param {string} type - 操作类型\r\n     */\r\n    addOrUpdateHandler(id, type) {\r\n      this.showFlag = false;\r\n      this.addOrUpdateFlag = true;\r\n      if (type != 'info') {\r\n        type = 'else';\r\n      }\r\n      this.$nextTick(() => {\r\n        this.$refs.addOrUpdate.init(id, type);\r\n      });\r\n    },\r\n\r\n    /**\r\n     * 删除处理方法\r\n     * @param {number|string} id - 要删除的记录ID\r\n     */\r\n    deleteHandler(id) {\r\n      var ids = id ? [Number(id)] : this.dataListSelections.map(item => {\r\n        return Number(item.id);\r\n      });\r\n\r\n      this.$confirm(`确定进行[${id ? \"删除\" : \"批量删除\"}]操作?`, \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\"\r\n      }).then(() => {\r\n        this.$http({\r\n          url: \"jiadianOrder/delete\",\r\n          method: \"post\",\r\n          data: ids\r\n        }).then(({data}) => {\r\n          if(data && data.code === 0){\r\n            this.$message({\r\n              message: \"操作成功\",\r\n              type: \"success\",\r\n              duration: 1500,\r\n              onClose: () => {\r\n                this.search();\r\n              }\r\n            });\r\n          }else{\r\n            this.$message.error(data.msg);\r\n          }\r\n        });\r\n      });\r\n    },\r\n\r\n    /**\r\n     * 打开发货模态框\r\n     * @param {number} id - 订单ID\r\n     */\r\n    openDeliver(id) {\r\n      this.jiadianOrderCourierNumber = null;\r\n      this.jiadianOrderCourierName = null;\r\n      this.deliverId = id;\r\n      this.deliverVisible = true;\r\n    },\r\n\r\n    /**\r\n     * 发货处理方法\r\n     */\r\n    deliver() {\r\n      let _this = this;\r\n      // 验证输入\r\n      if(!this.jiadianOrderCourierNumber) {\r\n        return this.$message.error(\"订单快递单号不能为空\");\r\n      }\r\n      if(!this.jiadianOrderCourierName) {\r\n        return this.$message.error(\"快递公司不能为空\");\r\n      }\r\n\r\n      _this.$confirm(`确定要发货吗?`, \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\"\r\n      }).then(() => {\r\n        let id = _this.deliverId;\r\n        _this.$http({\r\n          url: \"jiadianOrder/deliver?id=\"+id+\r\n              \"&jiadianOrderCourierNumber=\"+_this.jiadianOrderCourierNumber+\r\n              \"&jiadianOrderCourierName=\"+_this.jiadianOrderCourierName,\r\n          method: \"post\"\r\n        }).then(({ data }) => {\r\n          if (data && data.code === 0) {\r\n            _this.$message({\r\n              message:\"操作成功\",\r\n              type: \"success\",\r\n              duration: 1500,\r\n              onClose: () => {\r\n                this.search();\r\n                _this.deliverVisible = false;\r\n              }\r\n            });\r\n          } else {\r\n            _this.$message.error(data.msg);\r\n            _this.deliverVisible = false;\r\n          }\r\n        });\r\n        _this.deliverVisible = false;\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* 主内容容器样式 */\r\n.main-content {\r\n  padding: 20px;\r\n  background-color: #f5f5f5;\r\n}\r\n\r\n/* 表单内容样式 */\r\n.form-content {\r\n  padding: 20px;\r\n  background-color: #fff;\r\n  border-radius: 5px;\r\n  box-shadow: 0 0 5px rgba(0,0,0,0.1);\r\n  margin-bottom: 20px;\r\n}\r\n\r\n/* 搜索条件行样式 */\r\n.slt {\r\n  margin: 0 !important;\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: flex-start;\r\n}\r\n\r\n/* 操作按钮行样式 */\r\n.ad {\r\n  margin: 0 !important;\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: flex-start;\r\n}\r\n\r\n/* 表格内容区域样式 */\r\n.table-content {\r\n  padding: 20px;\r\n  background-color: #fff;\r\n  border-radius: 5px;\r\n  box-shadow: 0 0 5px rgba(0,0,0,0.1);\r\n}\r\n\r\n/* 表格样式 */\r\n.tables {\r\n  width: 100%;\r\n  font-size: 14px;\r\n}\r\n\r\n/* 表格表头样式 */\r\n.tables >>> .el-table__header th {\r\n  background-color: var(--publicSubColor);\r\n  color: #fff !important;\r\n  font-weight: bold;\r\n}\r\n\r\n/* 表格斑马线样式 - 奇数行 */\r\n.tables >>> .el-table__body tr:nth-child(odd) {\r\n  background-