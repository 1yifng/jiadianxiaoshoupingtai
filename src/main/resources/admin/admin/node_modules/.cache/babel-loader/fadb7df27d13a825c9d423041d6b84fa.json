bel=\"操作\">\r\n            <template slot-scope=\"scope\">\r\n              <!-- 详情按钮 -->\r\n              <el-button\r\n                  type=\"success\"\r\n                  icon=\"el-icon-tickets\"\r\n                  size=\"mini\"\r\n                  @click=\"addOrUpdateHandler(scope.row.id,'info')\">详情</el-button>\r\n\r\n              <!-- 修改按钮 -->\r\n              <el-button\r\n                  type=\"primary\"\r\n                  icon=\"el-icon-edit\"\r\n                  size=\"mini\"\r\n                  @click=\"addOrUpdateHandler(scope.row.id)\">修改</el-button>\r\n\r\n              <!-- 删除按钮 -->\r\n              <el-button\r\n                  type=\"danger\"\r\n                  icon=\"el-icon-delete\"\r\n                  size=\"mini\"\r\n                  @click=\"deleteHandler(scope.row.id)\">删除</el-button>\r\n\r\n              <!-- 重置密码按钮 -->\r\n              <el-button\r\n                  type=\"success\"\r\n                  icon=\"el-icon-tickets\"\r\n                  size=\"mini\"\r\n                  @click=\"resetPassword(scope.row.id)\">重置密码</el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n\r\n        <!-- 分页组件 -->\r\n        <el-pagination\r\n            class=\"pagination-content\"\r\n            @size-change=\"sizeChangeHandle\"\r\n            @current-change=\"currentChangeHandle\"\r\n            :current-page=\"pageIndex\"\r\n            :page-sizes=\"[10, 20, 50, 100]\"\r\n            :page-size=\"pageSize\"\r\n            :total=\"totalPage\"\r\n            layout=\"total, sizes, prev, pager, next, jumper\"\r\n            background>\r\n        </el-pagination>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 添加/修改页面 -->\r\n    <add-or-update\r\n        v-if=\"addOrUpdateFlag\"\r\n        :parent=\"this\"\r\n        ref=\"addOrUpdate\">\r\n    </add-or-update>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AddOrUpdate from \"./add-or-update\";\r\n\r\nexport default {\r\n  components: {\r\n    AddOrUpdate,\r\n  },\r\n  data() {\r\n    return {\r\n      // 搜索表单数据\r\n      searchForm: {\r\n        yonghuName: \"\",  // 用户姓名\r\n        sexTypes: \"\",    // 性别\r\n      },\r\n\r\n      // 表格数据\r\n      dataList: [],          // 用户数据列表\r\n      pageIndex: 1,          // 当前页码\r\n      pageSize: 10,          // 每页条数\r\n      totalPage: 0,          // 总条数\r\n      dataListLoading: false,// 加载状态\r\n      dataListSelections: [],// 选中项\r\n\r\n      // 页面显示控制\r\n      showFlag: true,        // 是否显示列表页\r\n      addOrUpdateFlag: false,// 是否显示添加/修改页\r\n\r\n      // 用户信息\r\n      sessionTable: \"\",      // 当前登录用户表名\r\n      role: \"\",              // 用户角色\r\n      userId: \"\",            // 用户ID\r\n\r\n      // 下拉选项数据\r\n      sexTypesSelectSearch: [],  // 性别下拉选项\r\n    };\r\n  },\r\n  created() {\r\n    // 初始化获取数据\r\n    this.init();\r\n    this.getDataList();\r\n\r\n    // 获取当前登录用户信息\r\n    this.sessionTable = this.$storage.get(\"sessionTable\");\r\n    this.role = this.$storage.get(\"role\");\r\n    this.userId = this.$storage.get(\"userId\");\r\n  },\r\n  methods: {\r\n    // 初始化方法\r\n    init() {\r\n      // 可以在这里初始化一些数据\r\n    },\r\n\r\n    // 获取用户列表数据\r\n    getDataList() {\r\n      this.dataListLoading = true;\r\n      let params = {\r\n        page: this.pageIndex,\r\n        limit: this.pageSize,\r\n        sort: 'id',\r\n      };\r\n\r\n      // 添加搜索条件到参数\r\n      if (this.searchForm.yonghuName) {\r\n        params['yonghuName'] = '%' + this.searchForm.yonghuName + '%';\r\n      }\r\n      if (this.searchForm.sexTypes) {\r\n        params['sexTypes'] = this.searchForm.sexTypes;\r\n      }\r\n\r\n      // 只查询未删除的用户\r\n      params['yonghuDelete'] = 1;\r\n\r\n      // 发送请求获取用户数据\r\n      this.$http({\r\n        url: \"yonghu/page\",\r\n        method: \"get\",\r\n        params: params\r\n      }).then(({data}) => {\r\n        if(data && data.code === 0){\r\n          this.dataList = data.data.list;\r\n          this.totalPage = data.data.total;\