>\r\n    <!-- 使用Element UI的表单组件 -->\r\n    <el-form\r\n        class=\"detail-form-content\"\r\n        ref=\"ruleForm\"\r\n        :model=\"ruleForm\"\r\n        :rules=\"rules\"\r\n        label-width=\"auto\">\r\n      <el-row>\r\n        <!-- 商品选择列 -->\r\n<!--        <el-col :span=\"12\" v-if=\"sessionTable !='jiadian'\">-->\r\n<!--          <el-form-item class=\"select\" v-if=\"type!='info'\" label=\"商品\" prop=\"jiadianId\">-->\r\n<!--            <el-select-->\r\n<!--                v-model=\"ruleForm.jiadianId\"-->\r\n<!--                :disabled=\"ro.jiadianId\"-->\r\n<!--                filterable-->\r\n<!--                placeholder=\"请选择商品\"-->\r\n<!--                @change=\"jiadianChange\">-->\r\n<!--              <el-option-->\r\n<!--                  v-for=\"(item,index) in jiadianOptions\"-->\r\n<!--                  :key=\"item.id\"-->\r\n<!--                  :label=\"item.jiadianName\"-->\r\n<!--                  :value=\"item.id\">-->\r\n<!--              </el-option>-->\r\n<!--            </el-select>-->\r\n<!--          </el-form-item>-->\r\n<!--        </el-col>-->\r\n\r\n        <!-- 商品名称显示列 -->\r\n        <el-col :span=\"12\" v-if=\"sessionTable !='jiadian'\">\r\n          <el-form-item class=\"input\" v-if=\"type!='info'\" label=\"商品名称\" prop=\"jiadianName\" >\r\n            <el-input\r\n                v-model=\"jiadianForm.jiadianName\"\r\n                placeholder=\"商品名称\"\r\n                clearable\r\n                readonly></el-input>\r\n          </el-form-item>\r\n          <div v-else-if=\"type=='info'\">\r\n            <el-form-item class=\"input\" label=\"商品名称\" prop=\"jiadianName\">\r\n              <el-input\r\n                  v-model=\"ruleForm.jiadianName\"\r\n                  placeholder=\"商品名称\"\r\n                  readonly></el-input>\r\n            </el-form-item>\r\n          </div>\r\n        </el-col>\r\n\r\n        <!-- 用户选择列 -->\r\n<!--        <el-col :span=\"12\" v-if=\"sessionTable !='yonghu'\">-->\r\n<!