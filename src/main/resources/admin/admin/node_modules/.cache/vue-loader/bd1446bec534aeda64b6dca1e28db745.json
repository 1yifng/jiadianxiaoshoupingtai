l-input v-model=\"ruleForm.jiadianOrderValue\" readonly></el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n\r\n        <!-- 商品信息 -->\r\n        <el-col :span=\"12\">\r\n          <el-form-item label=\"商品名称\">\r\n            <el-input v-model=\"ruleForm.jiadianName\" readonly></el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"12\">\r\n          <el-form-item label=\"商品图片\" v-if=\"ruleForm.jiadianPhoto\">\r\n            <img\r\n                v-for=\"(item,index) in ruleForm.jiadianPhoto.split(',')\"\r\n                :src=\"$base.url+item\"\r\n                width=\"100\"\r\n                height=\"100\"\r\n                :key=\"index\">\r\n          </el-form-item>\r\n        </el-col>\r\n\r\n        <!-- 收货信息 -->\r\n        <el-col :span=\"12\">\r\n          <el-form-item label=\"收货人\">\r\n            <el-input v-model=\"ruleForm.addressName\" readonly></el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"12\">\r\n          <el-form-item label=\"联系电话\">\r\n            <el-input v-model=\"ruleForm.addressPhone\" readonly></el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"24\">\r\n          <el-form-item label=\"收货地址\">\r\n            <el-input v-model=\"ruleForm.addressDizhi\" readonly type=\"textarea\"></el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n\r\n        <!-- 订单金额 -->\r\n        <el-col :span=\"12\">\r\n          <el-form-item label=\"实付金额\">\r\n            <el-input v-model=\"ruleForm.jiadianOrderTruePrice\" readonly></el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n      </el-row>\r\n\r\n      <el-form-item>\r\n        <el-button @click=\"back\" class=\"backbutton\">返回</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      id: '',\r\n      ruleForm: {\r\n        jiadianOrderUuidNumber: '',\r\n        jiadianOrderValue: '',\r\n        jiadianName: '',\r\n        jiadianPhoto: '',\r\n        addressName: '',\r\n        addressPhone: '',\r\n        addressDizhi: '',\r\n        jiadianOrderTruePrice: ''\r\n      }\r\n    };\r\n  },\r\n  props: [\"parent\"],\r\n  methods: {\r\n    // 初始化方法（保持与原调用方式一致）\r\n    init(id) {\r\n      this.id = id;\r\n      this.loadDetail(id);\r\n    },\r\n\r\n    // 加载详情数据（保持原有请求方式）\r\n    loadDetail(id) {\r\n      this.$http.get(`jiadianOrder/info/${id}`).then(({ data }) => {\r\n        if (data.code === 0) {\r\n          this.ruleForm = data.data;\r\n          // 转换状态码为文字（示例）\r\n          if (data.data.jiadianOrderTypes) {\r\n            const statusMap = { '101':'已支付', '102':'已发货', '103':'已完成' };\r\n            this.ruleForm.jiadianOrderValue = statusMap[data.data.jiadianOrderTypes] || data.data.jiadianOrderTypes;\r\n          }\r\n        }\r\n      });\r\n    },\r\n\r\n    // 返回列表页（保持原有父组件通信方式）\r\n    back() {\r\n      this.parent.showFlag = true;\r\n      this.parent.addOrUpdateFlag = false;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.addEdit-block {\r\n  padding: 20px;\r\n}\r\n.detail-form-content {\r\n  background: #fff;\r\n  padding: 20px;\r\n  border-radius: 4px;\r\n}\r\nimg {\r\n  margin-right: 10px;\r\n  border: 1px solid #eee;\r\n}\r\n\r\n.backbutton{\r\n  background-color: #8989c3;\r\n  border-color:#7fcfef ;\r\n  border-width: 10px;\r\n  padding: 10px 20px;\r\n  font-size: 14px;\r\n  border-radius: 4px;\r\n}\r\n\r\n</style>"]}]}